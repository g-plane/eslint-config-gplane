  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "devDependencies": {
    "@types/jest": "^21.1.9",
    "eslint": "^4.13.1",
    "eslint-plugin-eslint-comments": "^2.0.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^22.0.3"
  },
  "peerDependencies": {
    "eslint": "^4.13.1"
  }
}
