{
  "name": "eslint-config-gplane",
  "version": "1.6.0",
  "description": "Shareable ESLint config for GPlane's projects",
  "main": "index.js",
  "author": "gplane",
  "repository": {
    "type": "git",
    "url": "https://github.com/g-plane/eslint-config-gplane"
  },
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "lint": "eslint __tests__/ ./*.js",
